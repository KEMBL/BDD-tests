{
  "name": "cucumber-ts-starter",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm-run-all cucumber report",
    "cucumber": "./node_modules/.bin/cucumber-js.cmd features/**/*.feature --logLevel=error --require step-definitions/**/*.ts --require hooks/**/*.ts  --require-module ts-node/register --format-options '{\"snippetInterface\": \"async-await\"}' --format json:reports/cucumber-report.json --format summary",
    "typescript:init": "tsc --init",
    "tslint:init": "tslint --init",
    "tsc": "tsc",
    "report": "node reporters/cucumber-html-reporter.js",
    "lint": "tslint --format stylish --config tslint.json -e **/*.d.ts **/*.ts",
    "prebuild": "rimraf build",
    "build": "npm-run-all tsc lint"
  },
  "author": "Henri d'Orgeval",
  "license": "ISC",
  "dependencies": {
    "@types/chai": "4.1.2",
    "@types/cucumber": "4.0.1",
    "@types/loglevel": "1.5.3",
    "@types/minimist": "1.2.0",
    "@types/node": "9.4.6",
    "chai": "4.1.2",
    "cucumber": "4.0.0",
    "cucumber-html-reporter": "4.0.2",
    "jsome": "2.5.0",
    "minimist": "1.2.0",
    "npm-run-all": "4.1.2",
    "rimraf": "2.6.2",
    "ts-node": "5.0.1",
    "typescript": "2.7.2"
  },
  "devDependencies": {
    "eslint": "4.18.2",
    "tslint": "5.9.1"
  }
}
